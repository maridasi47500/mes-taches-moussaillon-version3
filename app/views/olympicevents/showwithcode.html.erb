<p id="notice"><%= notice %></p>
<h1><%=@edition.olympicgame.name%></h1>
<h2><%=l(@edition.opened, format: :long)%> - <%=l(@edition.closed,format: :long)%></h2>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>city</th>
      <th>country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= link_to(event.olympicevent.try(:name),editionshaveevent_path(event.id)) %></td>
        <td><%= event.city.try(:name) %></td>
        <td><%= event.country.try(:name) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

