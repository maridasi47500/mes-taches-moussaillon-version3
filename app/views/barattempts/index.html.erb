<p id="notice"><%= notice %></p>

<h1>Barattempts</h1>

<table>
  <thead>
    <tr>
      <th>Person</th>
      <th>User</th>
      <th>Sample</th>
      <th>Bar</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @barattempts.each do |barattempt| %>
      <tr>
        <td><%= barattempt.person_id %></td>
        <td><%= barattempt.user_id %></td>
        <td><%= barattempt.sample_id %></td>
        <td><%= barattempt.bar_id %></td>
        <td><%= link_to 'Show', barattempt %></td>
        <td><%= link_to 'Edit', edit_barattempt_path(barattempt) %></td>
        <td><%= link_to 'Destroy', barattempt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Barattempt', new_barattempt_path %>
