function chercher<%=t[0]%>(field,al){
if (field.value.trim() !== ''){
$.ajax({url: '<%=t[1]%>?name='+field.value, 
success:function(data){
al.innerHTML="<option></option>";
var value, id;
for (var i=0;i<data.length;i++){
id=data[i].id;
name=data[i].<%=t[3]%>||data[i].<%=t[3]%>_<%=I18n.locale%>||data[i].<%=t[3]%>_fr||data[i].<%=t[3]%>_en||data[i].<%=t[3]%>_de;
al.innerHTML+="<option value='"+id+"'>"+name+"</option>";
}
}});
}
};
function select<%=t[0]%>(field,greatpeople){
