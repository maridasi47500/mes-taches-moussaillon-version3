<%=content_for :mylittlecss do%>
.olympiccard {
padding: 10px;
margin: 10px;
border: dotted red 2px;
}
p, .card {
margin: 10px;
}
<%end%>

<p id="notice"><%= notice %></p>

<h1>Olympicpeople</h1>

<table>
  <thead>
    <tr>
      <th>game</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @olympicpeople.each do |olympicperson| %>
      <tr>
        <td><%= olympicperson.relationship %></td>
        <td><%= link_to 'Show', olympicperson %></td>
        <td><%= link_to 'Edit', edit_olympicperson_path(olympicperson) %></td>
        <td><%= link_to 'Destroy', olympicperson, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

