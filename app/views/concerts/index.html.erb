<p id="notice"><%= notice %></p>

<h1>Concerts</h1>

<table>
  <thead>
    <tr>
      <th>Name fr</th>
      <th>Name en</th>
      <th>Name de</th>
      <th>Description</th>
      <th>Start at</th>
      <th>End at</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @concerts.each do |concert| %>
      <tr>
        <td><%= concert.name_fr %></td>
        <td><%= concert.name_en %></td>
        <td><%= concert.name_de %></td>
        <td><%= concert.description %></td>
        <td><%= concert.start_at %></td>
        <td><%= concert.end_at %></td>
        <td><%= concert.description %></td>
        <td><%= link_to 'Show', concert %></td>
        <td><%= link_to 'Edit', edit_concert_path(concert) %></td>
        <td><%= link_to 'Destroy', concert, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Concert', new_concert_path %>
