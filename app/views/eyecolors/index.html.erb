<p id="notice"><%= notice %></p>

<h1>Eyecolors</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @eyecolors.each do |eyecolor| %>
      <tr>
        <td><%= eyecolor.name %></td>
        <td><%= link_to 'Show', eyecolor %></td>
        <td><%= link_to 'Edit', edit_eyecolor_path(eyecolor) %></td>
        <td><%= link_to 'Destroy', eyecolor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Eyecolor', new_eyecolor_path %>
