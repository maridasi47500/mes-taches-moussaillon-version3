<p id="notice"><%= notice %></p>

<h1>Facebook Requests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @facebook_requests.each do |facebook_request| %>
      <tr>
        <td><%= facebook_request.name %></td>
        <td><%= link_to 'Show', facebook_request %></td>
        <td><%= link_to 'Edit', edit_facebook_request_path(facebook_request) %></td>
        <td><%= link_to 'Destroy', facebook_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Facebook Request', new_facebook_request_path %>
