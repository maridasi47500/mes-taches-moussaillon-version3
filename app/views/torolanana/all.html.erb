
    <% if @sectors.length > 0 %>

<h1>Filieres correspondant a votre recherche</h1>

<table>
  <thead>
    <tr>
      <th>Quelle filiere poursuivre ?</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sectors.each do |sector| %>
      <tr>
        <td><%= sector.name %></td>
        <td><%= link_to 'Show', sector %></td>
        <td><%= link_to 'Edit', edit_sector_path(sector) %></td>
        <td><%= link_to 'Destroy', sector, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%else%>
<h1>Aucune filiere ne correspond a votre recherche</h1>

<%end%>
<%=link_to "Revenir a la recherche d'une filiere", torolanana_path%> 
