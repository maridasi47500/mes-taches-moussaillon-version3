<p id="notice"><%= notice %></p>

<h1>Lesson Recordings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lesson_recordings.each do |lesson_recording| %>
      <tr>
        <td><%= link_to 'Show', lesson_recording %></td>
        <td><%= link_to 'Edit', edit_lesson_recording_path(lesson_recording) %></td>
        <td><%= link_to 'Destroy', lesson_recording, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lesson Recording', new_lesson_recording_path %>
