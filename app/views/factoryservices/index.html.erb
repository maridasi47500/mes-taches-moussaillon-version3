<p id="notice"><%= notice %></p>

<h1>Factoryservices</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @factoryservices.each do |factoryservice| %>
      <tr>
        <td><%= factoryservice.name %></td>
        <td><%= factoryservice.image %></td>
        <td><%= link_to 'Show', factoryservice %></td>
        <td><%= link_to 'Edit', edit_factoryservice_path(factoryservice) %></td>
        <td><%= link_to 'Destroy', factoryservice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Factoryservice', new_factoryservice_path %>
