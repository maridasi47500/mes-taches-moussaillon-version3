<%=form_with url: armeegrade_path do |form|%>
<input id="imagearmegrade" type="image" width="100" height="100" alt="Login"
       src="/assets/gradearmeeterre.png">

<%end%>
<%current_user.userhavearmygrades.each do |t|%>
<%=button_to t.armygrade.nom+' X', armeegradedelete_path(t.id), method: :delete, data: { confirm: 'Are you sure?' } %>
<%end%>




