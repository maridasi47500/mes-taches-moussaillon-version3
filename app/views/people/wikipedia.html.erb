  </div>
        <div class="field">
    <%= wikipedia.label :instruments %>
    <%= wikipedia.text_field :instruments %>

  </div>
          <div class="field">
    <%= wikipedia.label :active_years %>
    <%= wikipedia.date_field :active_years %>

  </div>
            <div class="field">
    <%= wikipedia.label :influences %>
    <%= wikipedia.text_field :influences %>

  </div>
              <div class="field">
    <%= wikipedia.label :official_site %>
    <%= wikipedia.text_field :official_site %>

  </div>
  <%= wikipedia.fields_for :photos do |photo|%>
  <div class="field">

<%= photo.label :file, "Nouvelle photo"%>
    <%= photo.file_field :file %>
  </div>

  <div class="field">
<%= photo.label :title,"Titre"%>
    <%= photo.text_field :title %>
  </div>

<%end%>
<%end%>

  <div class="actions">
    <%= form.submit "Add Those Jingles" %>
  </div>

<%end%>
