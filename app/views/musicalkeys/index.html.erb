<p id="notice"><%= notice %></p>

<h1>Musicalkeys</h1>

<table>
  <thead>
    <tr>
      <th>Name en</th>
      <th>Name fr</th>
      <th>Name de</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @musicalkeys.each do |musicalkey| %>
      <tr>
        <td><%= musicalkey.name_en %></td>
        <td><%= musicalkey.name_fr %></td>
        <td><%= musicalkey.name_de %></td>
        <td><%= link_to 'Show', musicalkey %></td>
        <td><%= link_to 'Edit', edit_musicalkey_path(musicalkey) %></td>
        <td><%= link_to 'Destroy', musicalkey, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Musicalkey', new_musicalkey_path %>
