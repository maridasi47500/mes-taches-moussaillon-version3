<p id="notice"><%= notice %></p>

<h1>Styleofmusics</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @styleofmusics.each do |styleofmusic| %>
      <tr>
        <td><%= styleofmusic.name %></td>
        <td><%= link_to 'Show', styleofmusic %></td>
        <td><%= link_to 'Edit', edit_styleofmusic_path(styleofmusic) %></td>
        <td><%= link_to 'Destroy', styleofmusic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Styleofmusic', new_styleofmusic_path %>
