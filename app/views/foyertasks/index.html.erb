<p id="notice"><%= notice %></p>

<h1>Foyertasks</h1>

<table>
  <thead>
    <tr>
      <th>User1</th>
      <th>User2</th>
      <th>Task</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foyertasks.each do |foyertask| %>
      <tr>
        <td><%= foyertask.user1 %></td>
        <td><%= foyertask.user2 %></td>
        <td><%= foyertask.task %></td>
        <td><%= link_to 'Show', foyertask %></td>
        <td><%= link_to 'Edit', edit_foyertask_path(foyertask) %></td>
        <td><%= link_to 'Destroy', foyertask, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foyertask', new_foyertask_path %>
