<p id="notice"><%= notice %></p>

<h1>Social movements</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @socialmovements.each do |socialmovement| %>
      <tr>
        <td><%= socialmovement.name %></td>
        <td><%= link_to 'Show', socialmovement %></td>
        <td><%= link_to 'Edit', edit_socialmovement_path(socialmovement) %></td>
        <td><%= link_to 'Destroy', socialmovement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Socialmovement', new_socialmovement_path %>
