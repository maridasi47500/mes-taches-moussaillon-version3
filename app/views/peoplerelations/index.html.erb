<p id="notice"><%= notice %></p>

<h1>Peoplerelations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @peoplerelations.each do |peoplerelation| %>
      <tr>
        <td><%= peoplerelation.name %></td>
        <td><%= link_to 'Show', peoplerelation %></td>
        <td><%= link_to 'Edit', edit_peoplerelation_path(peoplerelation) %></td>
        <td><%= link_to 'Destroy', peoplerelation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Peoplerelation', new_peoplerelation_path %>
