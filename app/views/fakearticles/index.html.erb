<p id="notice"><%= notice %></p>

<h1>Fakearticles</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Authorname</th>
      <th>Authorpseudo</th>
      <th>Content</th>
      <th>Category</th>
      <th>Pic</th>
      <th>Authorpic</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fakearticles.each do |fakearticle| %>
      <tr>
        <td><%= fakearticle.title %></td>
        <td><%= fakearticle.authorname %></td>
        <td><%= fakearticle.authorpseudo %></td>
        <td><%= fakearticle.content %></td>
        <td><%= fakearticle.category %></td>
        <td><%= fakearticle.pic %></td>
        <td><%= fakearticle.authorpic %></td>
        <td><%= link_to 'Show', fakearticle %></td>
        <td><%= link_to 'Edit', edit_fakearticle_path(fakearticle) %></td>
        <td><%= link_to 'Destroy', fakearticle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fakearticle', new_fakearticle_path %>
