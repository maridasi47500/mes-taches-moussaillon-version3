<p id="notice"><%= notice %></p>

<h1>Reactions Texts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reactions_texts.each do |reactions_text| %>
      <tr>
        <td><%= reactions_text.name %></td>
        <td><%= link_to 'Show', reactions_text %></td>
        <td><%= link_to 'Edit', edit_reactions_text_path(reactions_text) %></td>
        <td><%= link_to 'Destroy', reactions_text, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reactions Text', new_reactions_text_path %>
<%=render "indexpage/pagination",g:params[:page].to_i, modelname: ReactionsText,mypath: reactions_texts_path+'?'%>

