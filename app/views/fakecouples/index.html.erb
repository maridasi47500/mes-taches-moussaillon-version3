<p id="notice"><%= notice %></p>

<h1>Fakecouples</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fakecouples.each do |fakecouple| %>
      <tr>
        <td><%= link_to 'Show', fakecouple %></td>
        <td><%= link_to 'Edit', edit_fakecouple_path(fakecouple) %></td>
        <td><%= link_to 'Destroy', fakecouple, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fakecouple', new_fakecouple_path %>
