<p id="notice"><%= notice %></p>

<h1>Create Webpages</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @create_webpages.each do |create_webpage| %>
      <tr>
        <td><%= create_webpage.title %></td>
        <td><%= create_webpage.url %></td>
        <td><%= link_to 'Show', create_webpage %></td>
        <td><%= link_to 'Edit', edit_create_webpage_path(create_webpage) %></td>
        <td><%= link_to 'Destroy', create_webpage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Create Webpage', new_create_webpage_path %>
