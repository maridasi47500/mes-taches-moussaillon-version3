<p id="notice"><%= notice %></p>

<h1>Mymaps</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Width</th>
      <th>Height</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mymaps.each do |mymap| %>
      <tr>
        <td><%= mymap.image %></td>
        <td><%= mymap.width %></td>
        <td><%= mymap.height %></td>
        <td><%= link_to 'Show', mymap %></td>
        <td><%= link_to 'Edit', edit_mymap_path(mymap) %></td>
        <td><%= link_to 'Destroy', mymap, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mymap', new_mymap_path %>
