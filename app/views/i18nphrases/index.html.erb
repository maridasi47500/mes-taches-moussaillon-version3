<p id="notice"><%= notice %></p>

<h1>I18nphrases</h1>

<table>
  <thead>
    <tr>
      <th>Name en</th>
      <th>Name fr</th>
      <th>Name de</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @i18nphrases.each do |i18nphrase| %>
      <tr>
        <td><%= i18nphrase.content %></td>
        <%if i18nphrase.lg%>
<td>
<%=image_tag "/assets/202105071417260200.gif",class:"imageprofilethread"%>(lgbt)
</td>
<%end%>
<%if i18nphrase.lsb%>
<td>
<%=image_tag "/assets/202105071417260200.gif",class:"imageprofilethread"%>(lesbian)
</td>
<%end%>
<%if i18nphrase.gf%>
<td>
<%=image_tag "/assets/202105071417260200.gif",class:"imageprofilethread"%>(girlfriend)
</td>
<%end%>

        <td><%= link_to 'Show', i18nphrase %></td>
        <td><%= link_to 'Edit', edit_i18nphrase_path(i18nphrase) %></td>
        <td><%= link_to 'Destroy', i18nphrase, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New I18nphrase', new_i18nphrase_path %>
<%=render "indexpage/pagination",g:params[:page].to_i, modelname: I18nphrase,mypath: i18nphrases_path+'?'%>

