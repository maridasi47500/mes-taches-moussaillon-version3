<p id="notice"><%= notice %></p>

<h1>Makeupstyles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @makeupstyles.each do |makeupstyle| %>
      <tr>
        <td><%= makeupstyle.name %></td>
        <td><%= link_to 'Show', makeupstyle %></td>
        <td><%= link_to 'Edit', edit_makeupstyle_path(makeupstyle) %></td>
        <td><%= link_to 'Destroy', makeupstyle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Makeupstyle', new_makeupstyle_path %>
