<%=form.fields_for :episodes, episodes do |f|%>
<div class="field">
<%=f.label :date%>
<%=f.date_field :date%>
</div>

<div class="field">
<%=f.label :saison%>
<%=f.number_field :saison%>
</div>
<div class="field">
<%=f.label :ep%>
<%=f.number_field :ep%>
</div>
